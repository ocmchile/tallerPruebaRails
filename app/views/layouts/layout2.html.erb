<!DOCTYPE html>
<html>
<head>
  <title>TallerPruebaRails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <!-- Bootstrap Core CSS -->
  <link href="<%= asset_path('vendor/bootstrap/css/bootstrap.min.css') %>" rel="stylesheet">

  <!-- Custom Fonts -->
  <link href="<%= asset_path('vendor/font-awesome/css/font-awesome.min.css') %>" rel="stylesheet" type="text/css">
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

  <!-- Plugin CSS -->
  <link href="<%= asset_path('vendor/magnific-popup/magnific-popup.css') %>" rel="stylesheet">

  <!-- Theme CSS -->
  <link href="<%= asset_path('css/creative.min.css') %>" rel="stylesheet">

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body id="page-top">

<%= yield %>

<!-- jQuery -->
<script src="<%= asset_path('vendor/jquery/jquery.min.js') %>"></script>

<!-- Bootstrap Core JavaScript -->
<script src="<%= asset_path('vendor/bootstrap/js/bootstrap.min.js') %>"></script>

<!-- Plugin JavaScript -->
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
<script src="<%= asset_path('vendor/scrollreveal/scrollreveal.min.js') %>"></script>
<script src="<%= asset_path('vendor/magnific-popup/jquery.magnific-popup.min.js') %>"></script>

<!-- Theme JavaScript -->
<script src="<%= asset_path('js/creative.min.js') %>"></script>

</body>
</html>
